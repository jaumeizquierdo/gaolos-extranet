@model ModuloClientesLibrary.ModuloClientesClienteFiscalViewModel
@{
    Layout = "~/Views/Shared/Modulo-Contactos/_Modulo-Contactos-Contacto.cshtml";
    ViewData["Title"] = "Acceso web";
    ViewData["Message"] = "Modulo de Contactos - Gaolos App v2017";
}

<div class="main-content m-0">

    <div class="container-fluid">
        <div class="row">
            <div class="col-12 header-section">
                <h2>@ViewData["Title"]</h2>
                @Html.Raw(ViewData["Migas"])
            </div>
        </div>
    </div>

    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">

                @if(Model.Model.Err.eserror)
{
    ViewData["err"] = Model.Model.Err.mensaje;
                @Html.Partial("AccesoDenegado");
}
else
{

                @* cabecera cliente *@

                @Html.Partial("Contacto-Cabecera")

                @* tabs *@

                @Html.Partial("Contacto-Tabs")

                @* Listado de direcciones *@

                <div class="row">
                    <div class="col-12">
                        @* Comercial *@
                        <div class="card card-table mb-3">
                            <div class="card-header card-header-options">
                                <span>Acceso web</span>
                            </div>

                            <div class="card-block">

                                <table class="table table-striped">
                                    <thead>
                                        <tr>
                                            <th class="w-10">NIC</th>
                                            <th>Nombre</th>
                                            <th class="w-10 text-center">Alta</th>
                                            <th class="w-10 text-center">Último acceso</th>
                                            <th>Usuario</th>
                                            <th class="w-5 text-center">Act.</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @for(int i = 0; i <5; i++)
                                        {
                                        <tr>
                                            <td>
                                                <p class="mb-0 fw-5">B60027182</p>
                                            </td>
                                            <td>
                                                <p class="mb-0 fw-5">Forcadell Fincas <i class="fa fa-home mdc-text-blue-grey-400" data-toggle="tooltip" data-placement="top" title="" data-original-title="Es administrador"></i></p>
                                                <a href="mailto:x.raventos@forcadell.com">x.raventos@forcadell.com</a>
                                            </td>
                                            <td class="text-center">
                                                <p class="mb-0">08/06/18</p>
                                            </td>
                                            <td class="text-center">
                                                <p class="mb-0">08/06/18</p>
                                            </td>
                                            <td>
                                                <p class="mb-0">Elisabet Ruiz</p>
                                            </td>
                                            <td class="text-center">
                                                <p class="mb-0">Sí</p>
                                            </td>
                                        </tr>

                                        }

                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>

                    <div class="col-3">

                    </div>

                </div>
}
            </div>
        </div>
    </div>
</div>

