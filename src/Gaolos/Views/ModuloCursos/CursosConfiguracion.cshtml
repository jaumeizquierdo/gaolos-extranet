<script>


    $(".col-conf figure").mouseover(function () {
        $(this).find("img").addClass("animated infinite bounce");
    });
    $(".col-conf figure").mouseout(function () {
        $(this).find("img").removeClass("animated infinite bounce");
    });
</script>