@model CursosLibrary.ModuloCursosCursosListadoViewModel
@{
    Layout = "~/Views/Shared/Modulo-Cursos/_Cursos.cshtml";
    ViewData["Title"] = "Cursos Configuracion";
    ViewData["Message"] = "Cursos - Gaolos App v2017";
}

<div class="main-content m-0" id="widget-area">

