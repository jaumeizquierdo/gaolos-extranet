                            <span>@ViewData["Title"]</span>
                            @Html.Partial("PanelActions")
                        </div>
                        <div class="card-block p-5">

                            <div class="row mb-4">
                                <div class="col-3 text-center col-conf">
                                    <figure>
                                        <a href="/modulo-cursos/configuracion/temarios">
